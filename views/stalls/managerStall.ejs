<%- include("../partials/header.ejs") -%>


<div class="container ">
    <div class="row">
        <%allStall.forEach(stall => {%>
        <div class="col-sm-6 col-lg-3 ">
            <a href="/stalls/<%= stall._id %>">
                <div class="card">
                    <img class="card-img-top" src="<%= stall.image %>" alt="<%= stall.name %>">
                    <div class="card-body">
                        <h4 class="card-title">
                            <p><%= stall.name %>
                        </h4>
                    </div>
                </div>
            </a>
        </div>
        <% });
    %>
    </div>
</div>

<%- include("../partials/footer.ejs") -%>